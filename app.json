{
  "expo": {
    "name": "Smooky",
    "slug": "Smooky",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/smooky.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/smooky-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bugrarslan.Smooky"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/smooky.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bugrarslan.Smooky"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f79c1cb-ad24-4970-85c9-8d070945f2b9"
      }
    }
  }
}
